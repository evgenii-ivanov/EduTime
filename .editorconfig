# EditorConfig is awesome: https://EditorConfig.org

# top-most EditorConfig file
root = true

[*]
end_of_line = crlf
insert_final_newline = true
trim_trailing_whitespace = true
charset = utf-8

[*.cs]
tab_width = 4
indent_style = space
indent_size = 4

[*.yml]
indent_style = space
tab_width = 2
indent_size = 2

[*.sh]
end_of_line = lf

[*.conf]
end_of_line = lf
